      <img src="{{ entry.image.one() }}" alt="{{ entry.title }}" class="project-image">
    <h1>{{ entry.title }}</h1>
    <div class="project-context">
      {{ entry.context }}
    </div>
    {% if entry.urllink %}
      <a href="{{ entry.urllink }}" target="_blank" class="project-link">Visit Project â†’</a>
    {% endif %}
  </article>
{% endblock %}