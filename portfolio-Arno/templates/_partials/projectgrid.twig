<ul class="project-grid">
    {% for project in craft.entries.section('projects').all() %}
        <li class="project">
            <a href="{{ project.url }}">
                <img src="{{ project.image.one().getUrl()}}" alt="{{ project.title }}">
                <h2>{{ project.title }}</h2>
            </a>
        </li>
    {% endfor %}

    <li><img src="./assets/icons/right-arrow.png"></li>
</ul>